			<smartForm:GroupElement>
				<smartField:SmartField value="{Itgrowhouse}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Itratetype}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Itfwdbill}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Itfwdkwh}"/>
			</smartForm:GroupElement>
		</smartForm:Group>
		<smartForm:Group label="Cause">
			<smartForm:GroupElement>
				<smartField:SmartField value="{Causegrp}"/>
				<smartField:SmartField value="{Causecode}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Causeshtxt}"/>
			</smartForm:GroupElement>
		</smartForm:Group>
	</smartForm:SmartForm>
</core:FragmentDefinition>
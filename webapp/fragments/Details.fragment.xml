<core:FragmentDefinition xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:layout="sap.ui.layout">
	<!--<Toolbar>-->
	<!--	<ToolbarSpacer/>-->
	<!--	<Button text="Put in Process" press="statusInProcess" ariaLabelledBy="editButtonLabel" tooltip="Set Status"/>-->
	<!--	<Button text="Postpone" press="statusPostpone" ariaLabelledBy="editButtonLabel" tooltip="Set Status"/>-->
	<!--	<Button text="Complete" press="statusComplete" ariaLabelledBy="editButtonLabel" tooltip="Set Status"/>-->
	<!--</Toolbar>-->
	<VBox class="sapUiSmallMargin">
		<FlexBox alignItems="Start" justifyContent="End">
			<layout:HorizontalLayout>
				<Button text="Edit" type="Emphasized" press="onPressDetails" ariaLabelledBy="editButtonLabel" tooltip="Edit Notification Header"/>
			</layout:HorizontalLayout>
		</FlexBox>
		<f:SimpleForm id="detailsFrom" editable="false" layout="ResponsiveGridLayout" singleContainerFullSize="false">
			<f:content>
				<!--header Information-->
				<core:Title text="Header"/>
				<Label text="Coding Code"/>
				<Text id="Cocode" text="{Codingcode}"/>
				<!--Causes Information-->
				<!--<core:Title text="Causes"/>-->
				<Label text="Cause Code"/>
				<Text id="Cacode" text="{Causecode}"/>
				<Label text="Cause Text"/>
				<Text id="Catxt" text="{Causeshtxt}"/>
				<!--Item Information-->
				<!--<core:Title text="Items"/>-->
				<Label text="Item Code"/>
				<Text id="Icode" text="{Itobjpartcode}"/>
				<Label text="Damage Group"/>
				<Text id="Dagrp" text="{Itdamagegrp}"/>
				<Label text="Damage Code"/>
				<Text id="Dacode" text="{Itdamagecode}"/>
				<core:Title/>
				<Label text="Grow House"/>
				<Text id="Growh" text="{Itgrowhouse}"/>
				<Label text="Rate Type"/>
				<Text id="Rtype" text="{Itratetype}"/>
				<Label text="12 Month Forward Billing $"/>
				<Text id="Mbill" text="{Itfwdbill}"/>
				<Label text="12 Month Forward KWH"/>
				<Text id="Mkwh" text="{Itfwdkwh}"/>
			</f:content>
		</f:SimpleForm>
	</VBox>
</core:FragmentDefinition>
{
	"_version": "1.9.0",
	"sap.app": {
		"id": "smud.pm.ZPM_RPDS_INVESTIGATIONS",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"dataSources": {
			"mainService": {
				"uri": "/sap/opu/odata/sap/ZPM_INVESTIGATIONS_SRV/",
				"type": "OData",
				"settings": {
					"odataVersion": "2.0",
					"localUri": "localService/metadata.xml",
					"annotations": []
				}
			},
			"localPT": {
				"uri": "model/partnerType.json",
				"type": "JSON"
			},
			"localAT": {
				"uri": "model/activityType.json",
				"type": "JSON"
			},
			"localCC": {
				"uri": "model/causeCode.json",
				"type": "JSON"
			},
			"localIC": {
				"uri": "model/itemCode.json",
				"type": "JSON"
			}
		},
		"sourceTemplate": {
			"id": "sap.ui.ui5-template-plugin.1worklist",
			"version": "1.52.7"
		}
	},
	"sap.ui": {
		"technology": "UI5",
		"icons": {
			"icon": "sap-icon://task",
			"favIcon": "",
			"phone": "",
			"phone@2": "",
			"tablet": "",
			"tablet@2": ""
		},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},
	"sap.ui5": {
		"rootView": {
			"viewName": "smud.pm.ZPM_RPDS_INVESTIGATIONS.view.App",
			"type": "XML",
			"async": true,
			"id": "app"
		},
		"dependencies": {
			"minUI5Version": "1.42.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {},
				"sap.f": {},
				"sap.ushell": {},
				"sap.collaboration": {
					"lazy": true
				}
			}
		},
		"contentDensities": {
			"compact": true,
			"cozy": true
		},
		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "smud.pm.ZPM_RPDS_INVESTIGATIONS.i18n.i18n"
				}
			},
			"": {
				"dataSource": "mainService",
				"preload": true
			},
			"partnerType": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "localPT"
			},
			"activityType": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "localAT"
			},
			"causeCode": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "localCC"
			},
			"itemCode": {
				"type": "sap.ui.model.json.JSONModel",
				"dataSource": "localIC"
			}
		},
		"services": {
			"ShellUIService": {
				"factoryName": "sap.ushell.ui5service.ShellUIService",
				"lazy": false,
				"settings": {
					"setTitle": "auto"
				}
			}
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "smud.pm.ZPM_RPDS_INVESTIGATIONS.view",
				"controlId": "app",
				"controlAggregation": "pages",
				"bypassed": {
					"target": ["notFound"]
				},
				"async": true
			},
			"routes": [{
				"pattern": "",
				"name": "worklist",
				"target": ["worklist"]
			}, {
				"pattern": "InvHeaderSet/{objectId}",
				"name": "object",
				"target": ["object"]
			}, {
				"name": "create",
				"pattern": "create/{equip}/{pri}/{code}",
				"titleTarget": "",
				"greedy": false,
				"target": ["create"]
			}],
			"targets": {
				"worklist": {
					"viewName": "Worklist",
					"viewId": "worklist",
					"viewLevel": 1,
					"title": "{i18n>worklistViewTitle}"
				},
				"object": {
					"viewName": "Object",
					"viewId": "object",
					"viewLevel": 2,
					"title": "{i18n>objectViewTitle}"
				},
				"objectNotFound": {
					"viewName": "ObjectNotFound",
					"viewId": "objectNotFound"
				},
				"notFound": {
					"viewName": "NotFound",
					"viewId": "notFound"
				},
				"create": {
					"viewType": "XML",
					"transition": "slide",
					"clearAggregation": true,
					"viewName": "Create",
					"title": "Create New Investigation",
					"viewId": "create",
					"viewLevel": 2
				}
			}
		}
	},
	"sap.platform.abap": {
		"uri": "/sap/bc/ui5_ui5/sap/zpm_rpds_inv/webapp",
		"_version": "1.1.0"
	}
}
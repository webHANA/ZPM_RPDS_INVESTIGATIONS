<core:FragmentDefinition xmlns="sap.m" xmlns:f="sap.ui.layout.form" xmlns:core="sap.ui.core" xmlns:smartForm="sap.ui.comp.smartform"
	xmlns:smartField="sap.ui.comp.smartfield">
	<smartForm:SmartForm id="smartForm" editTogglable="true" title="Investigation Details" flexEnabled="false">
		<smartForm:Group label="Header">
			<smartForm:GroupElement>
				<smartField:SmartField value="{Notifstatus}"/>
				<smartField:SmartField value="{Notifstatustxt}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Equipment}"/>
				<smartField:SmartField value="{Equipdesc}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Equipaddress}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Codinggrp}"/>
				<smartField:SmartField value="{Codingcode}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Priority}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Notifdate}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Notifreporter}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Notifcreatedby}"/>
			</smartForm:GroupElement>
		</smartForm:Group>
		<smartForm:Group label="Item">
			<smartForm:GroupElement>
				<smartField:SmartField value="{Itobjpartgrp}"/>
				<smartField:SmartField value="{Itobjpartcode}"/>
			</smartForm:GroupElement>
			<smartForm:GroupElement>
				<smartField:SmartField value="{Itdamagegrp}"/>
